{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(mkdir *)",
      "Bash(git init)",
      "Bash(git add *)",
      "Bash(git commit *)",
      "Read(~/Desktop/coding-agent/templates/*)"
    ],
    "deny": [
      "Read(.env)",
      "Read(.env.*)"
    ]
  }
}
